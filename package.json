{
  "name": "-event-management",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "vite && node src/backend/server.js",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "nodemon server.js"
  },
  "nodemonConfig": {
    "verbose": true,
    "watch": [
      "backend/server.js"
    ],
    "ext": "js",
    "delay": "3000",
    "ignore": [
      "logs/*"
    ]
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.2",
    "axios": "^1.6.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mysql": "^2.18.1",
    "nodemon": "^3.0.2",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-calendar": "^4.7.0",
    "react-clock": "^4.6.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.18.0",
    "react-table": "^7.8.0",
    "react-tabs": "^6.0.2",
    "react-time-picker": "^6.6.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "vite": "^4.4.5"
  }
}
